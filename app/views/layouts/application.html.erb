<!-- Devise User Auth -->
<div style='float:right;'>
  <div id='navi' style="">
  <% if user_signed_in? %>
    Signed in as <%= current_user.email %>. Not you?
        <%= link_to "Sign Out", destroy_user_session_path, {:style=>"color:white", :method => :delete} %>
  <% else %>
    <%= link_to "Sign Up", new_user_registration_path, :style=>"" %> or <%= link_to "Sign In", new_user_session_path, :style=>"" %>
  <% end %>
  </div>

  <% flash.each do |name, msg| %>
    <%= content_tag :div, msg, :id => "flash#{name}" %>
  <% end %>
</div>
<!-- End Devise Auth -->

